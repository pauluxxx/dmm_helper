<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Document</title>

    {{#each styles}}
        <link rel="stylesheet" href="{{this.href}}">
    {{/each}}
</head>
<body>
<header>
    <nav>
        <ul>
            {{#each navs}}
                <li><a href="{{this.href}}">{{this.text}}</a></li>
            {{/each}}
        </ul>
    </nav>
</header>
<section>
    <article>
        <header>
            <h1>{{title}}</h1>
        </header>
        <p>
            {{text_about}}
        </p>
        <form action="{{action}}">
            <div class="form-group"><input type="text"></div>
            <div class="form-group"><button onclick="console.log(btn_upload_url);">{{btn_upload}}</button></div>
        </form>
    </article>
</section>
</body>
</html>